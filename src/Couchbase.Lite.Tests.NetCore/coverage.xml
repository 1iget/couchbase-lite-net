<AnalyzeParams>
  <TargetExecutable>C:\Program Files\dotnet\dotnet.exe</TargetExecutable>
  <TargetArguments>xunit -Verbose</TargetArguments>
  <TargetWorkingDir>.</TargetWorkingDir>
  <Output>dotCover.html</Output>
  <ReportType>html</ReportType>
  <Filters>
    <ExcludeFilters>
    <FilterEntry>
      <ModuleMask>xunit*</ModuleMask>
    </FilterEntry>
    <FilterEntry>
      <ModuleMask>FluentAssertions*</ModuleMask>
    </FilterEntry>
    <FilterEntry>
      <ModuleMask>Couchbase.Lite.Support.NetDesktop</ModuleMask>
    </FilterEntry>
    <FilterEntry>
      <ModuleMask>Couchbase.Lite.Tests.NetCore</ModuleMask>
    </FilterEntry>
    <FilterEntry>
      <ModuleMask>LibGit2Sharp</ModuleMask>
    </FilterEntry>
    <FilterEntry>
      <ModuleMask>dotnet*</ModuleMask>
    </FilterEntry>
    <FilterEntry>
      <ModuleMask>SourceLink*</ModuleMask>
    </FilterEntry>
    <FilterEntry>
      <ClassMask>JetBrains.Annotations*</ClassMask>
    </FilterEntry>
    <FilterEntry>
      <ClassMask>ObjCRuntime*</ClassMask>
    </FilterEntry>
    <FilterEntry>
      <ClassMask>LiteCore.Util.Perf*</ClassMask>
    </FilterEntry>
    <FilterEntry>
      <ClassMask>LiteCore.Util.Hasher</ClassMask>
    </FilterEntry>
    <FilterEntry>
      <ClassMask>Couchbase.Lite.Util.CollectionDebuggerView*</ClassMask>
    </FilterEntry>
     <FilterEntry>
      <ClassMask>Couchbase.Lite.Util.CBDebug</ClassMask>
    </FilterEntry>
     <FilterEntry>
      <ClassMask>Couchbase.Lite.Logging*</ClassMask>
    </FilterEntry>
     <FilterEntry>
      <ClassMask>Couchbase.Lite.Extensions</ClassMask>
    </FilterEntry>
    <FilterEntry>
      <FunctionMask>Equals</FunctionMask>
    </FilterEntry>
     <FilterEntry>
      <FunctionMask>GetHashCode</FunctionMask>
    </FilterEntry>
     <FilterEntry>
      <FunctionMask>ToString</FunctionMask>
    </FilterEntry>
  </ExcludeFilters>
  </Filters>
</AnalyzeParams>